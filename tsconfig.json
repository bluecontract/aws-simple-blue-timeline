{
  "compilerOptions": {
    "lib": ["es2021"],
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "es2021",
    "sourceMap": true,
    "outDir": "dist/out",
    "rootDir": "./src",
    "baseUrl": "./src",
    "strict": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": false
  }
}
